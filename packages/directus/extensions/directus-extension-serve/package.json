{
  "name": "directus-extension-serve",
  "description": "Serve files or an app with directus by using filename or file paths.",
  "icon": "deployed_code",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmerget/directus-serve.git"
  },
  "homepage": "https://github.com/nmerget/directus-serve",
  "bugs": {
    "url": "https://github.com/nmerget/directus-serve/issues"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-endpoint"
  ],
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "docs"
  ],
  "directus:extension": {
    "type": "endpoint",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate"
  },
  "devDependencies": {
    "@directus/extensions": "^3.0.7",
    "@directus/extensions-sdk": "14.0.0",
    "@directus/sdk": "^20.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "express": "^5.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "compression": "^1.8.0"
  }
}
